generator client {
  provider = "prisma-client-js"
}

datasource db {  
  provider = "sqlite"

  url      = env("DATABASE_URL")
}

model CacheStore {
  id        String   @id @unique
  
  data      Json
    
  updatedAt DateTime @updatedAt
}

model Quest {
  id        String   @id @unique

  data      Json

  updatedAt DateTime @updatedAt
}